{
  "name": "pageon/expiring-token",
  "description": "A cryptographically safe token with a build in customizable expiration date",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "token",
    "expiring",
    "time sensitive",
    "password reset",
    "password forgotten",
    "php"
  ],
  "support": {
    "issues": "https://github.com/Pageon/ExpriringToken/issues"
  },
  "homepage": "https://pageon.github.io/ExpriringToken",
  "authors": [
    {
      "name": "Jelmer Prins",
      "email": "jelmer@pageon.be"
    }
  ],
  "require": {
    "php": "^7.0"
  },
  "config": {
    "platform": {
      "php": "7.0.0"
    }
  },
  "autoload": {
    "psr-4": {
      "Pageon\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pageon\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  }
}
